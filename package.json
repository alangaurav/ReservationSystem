{
  "name": "reservationsystem",
  "version": "1.0.0",
  "description": "Device reservation web app.",
  "main": "server-app.js",
  "scripts": {
    "compile:sass": "node-sass sass/main.scss sass/style.comp.css",
    "compress:css": "node-sass sass/style.comp.css public/style.css --output-style compressed",
    "build-css": "npm-run-all compile:sass compress:css",
    "run-server": "nodemon ./server.js"
  },
  "keywords": [
    "Web",
    "App",
    "Node",
    "Express",
    "CSS",
    "HTML"
  ],
  "author": "Alan Gaurav",
  "license": "MIT",
  "devDependencies": {
    "nodemon": "^2.0.4"
  },
  "dependencies": {
    "autoprefixer": "^10.0.0",
    "express": "^4.17.1",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.0.9",
    "postcss-cli": "^8.0.0"
  }
}
